{
  "name": "immutable-cursor",
  "version": "1.0.2",
  "description": "Immutable cursors incorporating the Immutable.js API interface over a Clojure-inspired atom",
  "main": "dist/cursor.js",
  "author": {
    "name": "James Hopkins",
    "url": "https://github.com/jameshopkins"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbadger/immutable-cursor.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bugs": {
    "url": "https://github.com/redbadger/immutable-cursor/issues"
  },
  "homepage": "https://github.com/redbadger/immutable-cursor",
  "dependencies": {
    "atom-store": "0.0.2",
    "immutable": "3.7.6"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "semistandard": "^11.0.0",
    "sinon": "^2.1.0",
    "snazzy": "^7.0.0"
  },
  "scripts": {
    "lint": "semistandard | snazzy",
    "compile": "babel src --out-dir dist",
    "test": "npm run lint && mocha --compilers js:babel-core/register",
    "prepublish": "npm run test && npm run compile"
  },
  "semistandard": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true
    },
    "ignore": [
      "dist",
      "lib",
      "coverage"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ]
  }
}
