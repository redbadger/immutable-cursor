{
  "name": "immutable-cursor",
  "version": "0.0.2",
  "description": "A fork of the Immutable.js cursor with more practical semantics",
  "main": "dist/cursor.js",
  "author": {
    "name": "James Hopkins",
    "url": "https://github.com/jameshopkins"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/redbadger/immutable-cursor.git"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "bugs": {
    "url": "https://github.com/redbadger/immutable-cursor/issues"
  },
  "homepage": "https://github.com/redbadger/immutable-cursor",
  "dependencies": {
    "immutable": "3.7.6"
  },
  "devDependencies": {
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.2.0",
    "eslint": "^1.4.1",
    "eslint-config-airbnb": "0.0.8",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.3.2",
    "sinon": "^1.15.4"
  },
  "scripts": {
    "postinstall": "if [ -z \"$CI\" ]; then chmod +x hooks/*; cd .git/hooks && ln -sf ../../hooks/* .; fi",
    "lint": "node_modules/.bin/eslint .",
    "compile": "babel src --out-dir dist",
    "test": "node_modules/.bin/mocha --compilers js:babel-core/register",
    "prepublish": "npm run compile",
    "coverage": "babel-node $(npm bin)/istanbul cover --report html _mocha -- --compilers js:babel-core/register test/cursor.js"
  }
}
